{
	"version": 2,
	"customTemplates": [
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"duotone": [
			],
			"gradients": [
				{
					"slug": "vertical-secondary-to-tertiary",
					"gradient": "linear-gradient(to bottom,var(--wp--preset--color--secondary) 0%,var(--wp--preset--color--tertiary) 100%)",
					"name": "Vertical secondary to tertiary"
				}
			],
			"palette": [
				{
					"slug": "foreground",
					"color": "#1f1717",
					"name": "Foreground"
				},
				{
					"slug": "background",
					"color": "#f6f6f5",
					"name": "Background"
				},
				{
					"slug": "primary",
					"color": "#4f4d49",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#FC8D68",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#F6F6F6",
					"name": "seto"
				}
			]
		},
		"custom": {
			"spacing": {
				"small": "max(1.25vw, 5vw)",
				"medium": "clamp(2vw, 8vw, calc(4 * var(--wp--style--block-gap)))",
				"large": "clamp(4vw, 10vw, 8vw)",
				"outer": "var(--wp--custom--spacing--small, 1.25vw)"
			},
			"typography": {
				"font-size": {
					"our-service-heading":"5.1vw",
					"huge": "clamp(2.25vw, 4vw, 2.75vw)",
					"gigantic": "clamp(2.75vw, 6vw, 3.25vw)",
					"colossal": "clamp(3.25vw, 8vw, 6.25vw)"
				},
				"line-height": {
					"tiny": 1.15,
					"small": 1.2,
					"medium": 1.4,
					"normal": 1.6
				}
			}
		},
		"spacing": {

			"units": [
				"%",
				"px",
				"em",
				"vw",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": false,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "\"Lato Black\", serif",
					"name": "Lato Black",
					"slug": "lato",
					"fontFace": [
						{
							"fontFamily": "Lato Black",
							"fontWeight": "200 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/lato/Lato-Regular.ttf" ]
						},
						{
							"fontFamily": "Lato-Italic",
							"fontWeight": "200 900",
							"fontStyle": "italic",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/lato/Lato-Black.ttf" ]
						}
					]
				},
				{
					"fontFamily": "\"Oswald\", serif",
					"name": "Oswald Black",
					"slug": "oswald",
					"fontFace": [
						{
							"fontFamily": "Oswald",
							"fontWeight": "400",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/oswald/Oswald-Regular.ttf" ]
						},
						{
							"fontFamily": "Lato Black",
							"fontWeight": "900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/oswald/Oswald-Bold.ttf" ]
						}
					]
				}

			],
			"fontSizes": [
				{
					"size": "1vw",
					"slug": "small"
				},
				{
					"size": "16px",
					"slug": "medium"
				},
				{
					"size": "1.75vw",
					"slug": "large"
				},
				{
					"size": "clamp(1.75vw, 3vw, 2.25vw)",
					"slug": "x-large"
				}
			]
		},
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1200px"
		}
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "25px"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"core/post-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"fontWeight": "300",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)"
				}
			},
			"core/post-comments": {
				"spacing": {
					"padding": {
						"top": "var(--wp--custom--spacing--small)"
					}
				}
			},
			"core/pullquote": {
				"border": {
					"width": "1px 0"
				}
			},
			"core/query-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--small)",
					"fontSize": "var(--wp--custom--typography--font-size--gigantic)"
				}
			},
			"core/quote": {
				"border": {
					"width": "1px"
				}
			},
			"core/site-title": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontStyle": "italic",
					"fontWeight": "normal"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--foreground)"
		},
		"elements": {
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"fontWeight": "900",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--custom--typography--font-size--colossal)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"fontWeight": "200 900",
					"lineHeight": "var(--wp--custom--typography--line-height--small)",
					"fontSize": "var(--wp--custom--typography--font-size--our-service-heading)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--Oswald)",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--custom--typography--font-size--huge)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--Oswald)",
					"fontWeight": "500",
					"lineHeight": "var(--wp--custom--typography--line-height--tiny)",
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"fontWeight": "400",
					"textTransform": "uppercase",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--oswald)",
					"fontWeight": "400",
					"textTransform": "uppercase",
					"lineHeight": "var(--wp--custom--typography--line-height--normal)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"button":{
				"typography":{
					"fontSize":"16px"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--foreground)"
				}
			}
		},
		"spacing": {
			"blockGap": "1.5vw"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--oswald)",
			"lineHeight": "var(--wp--custom--typography--line-height--normal)",
			"fontSize": "var(--wp--preset--font-size--medium)"
		}
	},
	"templateParts": [
		{
			"name": "header",
			"title": "Header",
			"area": "header"
		},
		{
			"name": "footer",
			"title": "Footer",
			"area": "footer"
		}
	]
}
